{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "usage",
            "modules": false
        }]
    ],

    "plugins": [
        ["@babel/plugin-transform-runtime", {
                "corejs": false,
                "helpers": false,
                "regenerator": false,
                "useESModules": false
            }
        ],["component", 
            {
              "libraryName": "mint-ui",
              "style": true
            }
          ]
    ],
    "comments": false

}